{
  "name": "node_base",
  "version": "0.0.1",
  "description": "Base code Nodejs",
  "main": "dist/server.js",
  "private": true,
  "engines": {
    "node": ">=14.17.0 <17"
  },
  "scripts": {
    "start": "pm2 start node-base.json",
    "stop": "pm2 stop node-base.json",
    "restart": "pm2 startOrRestart node-base.json --update-env",
    "reload": "pm2 startOrReload node-base.json --update-env",
    "docker:start": "pm2-runtime node-base.json",
    "dev": "nodemon",
    "ts-node": "node -r ts-node/register/transpile-only src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/config/mySqlTypeorm.ts",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js --config dist/config/mySqlTypeorm.js",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:migrate:prod": "npm run typeorm:prod -- migration:run",
    "typeorm:migrate:undo": "npm run typeorm -- migration:revert",
    "typeorm:migrate:prod:undo": "npm run typeorm:prod -- migration:revert",
    "typeorm:seed": "ts-node ./node_modules/typeorm/cli.js --config src/config/mySqlSeedTypeorm.ts migration:run",
    "typeorm:seed:undo": "ts-node ./node_modules/typeorm/cli.js --config src/config/mySqlSeedTypeorm.ts migration:revert",
    "typeorm:seed:prod": "node ./node_modules/typeorm/cli.js --config dist/config/mySqlSeedTypeorm.js migration:run",
    "typeorm:seed:prod:undo": "node ./node_modules/typeorm/cli.js --config dist/config/mySqlSeedTypeorm.js migration:revert",
    "lint": "npm run eslint && npm run prettier",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "pretty-quick": "pretty-quick --staged",
    "lint:quick:fix": "npm run eslint:fix && npm run pretty-quick",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "preversion": "npm run build:check",
    "version": "standard-version",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:peerbits-admin/propanebros.git"
  },
  "author": "Propane Developer <propane@gmail.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/peerbits-admin/propanebros/src/Api_Development/",
