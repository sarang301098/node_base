        <div class="row pb-2 pt-2" style="height:100%">
          <div class="col-6" style="align-self: center;">
            <div class="pl-2">
              <h2><b>Sample Images</b></h2>
            </div>
          </div>
        </div>
      </div>
      <div>
        <% for(let index = 0; index<singularDefectsData.length; index+=3) { %>
        <div class="custom_row">
          <div class="custom_column">
            <img src="<%= singularDefectsData[index] %>" alt="Snow" style="width: 100%">
          </div>
          <% if((index + 1) < (singularDefectsData.length - 1)) { %>
          <div class="custom_column">
            <img src="<%= singularDefectsData[index + 1] %>" alt="Snow" style="width: 100%">
          </div>
          <% } %>
          <% if((index + 2) < (singularDefectsData.length - 1)) { %>
          <div class="custom_column">
            <img src="<%= singularDefectsData[index + 2] %>" alt="Snow" style="width: 100%">
          </div>
          <% } %>
        </div>
        <% } %>
      </div>
    </div>
    <% } %>

    <% const stoppageHistoriesData = JSON.parse(stoppageHistories) %>
    <% if(stoppageHistoriesData.length) { %>
    <% stoppageHistoriesData.map((stoppageHistory, index) => { %>
    <div class="container-fluid" style="page-break-before: always;">
      <div class="custom-header" style="margin-top: 20px;">
        <div class="row">
          <div class="col-4">
            <div class="had-lft">
              <h2>Stop #<%= index + 1 %></h2>
              <div class="date"> <span><%= getFormattedTime(stoppageHistory.stopTimestamp) %></span></div>
            </div>
          </div>
          <div class="col-4">
            <div class="text-center roll-header">
              <div><h3>Roll Number : <%= stoppageHistory.poRollIndex ? stoppageHistory.poRollIndex : '---' %> </h3></div>
            </div>
          </div>
          <div class="col-4">
            <div class="had-right">
              <span style="padding-right: 15px;"> <b>Defect:</b></span>
              <div class="product_box">
                <h3><%= stoppageHistory.defectsName %></h3>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="schedule-table">
        <div class="inner-jacket">
          <% if(stoppageHistory.frameRequests.length) { %>
          <div class="time-col">
            <div class="slot-wrap slot-right-border">
              <div class="col-heading"> TIME </div>
              <% stoppageHistory.frameRequests.forEach((frameRequest, frameRequestIndex) => { %>
              <% const batch = getBatch(frameRequest); %>
              <% if(!batch) return; %>
              <% const previousBatch = frameRequestIndex !== 0 ? getBatch(stoppageHistory.frameRequests[frameRequestIndex - 1]) : null; %>
              <% const nextBatch = frameRequestIndex < stoppageHistory.frameRequests.length - 1 ? getBatch(stoppageHistory.frameRequests[frameRequestIndex + 1]) : null; %>
              <% const isDefectiveBatch = batch.isBatchDefective; %>
              <% const isPreviousBatchDefective = previousBatch ? previousBatch.isBatchDefective : false; %>
              <% const isNextBatchDefective = nextBatch ? nextBatch.isBatchDefective : false; %>
              <% className = "time-slot";%>
              <% if(isDefectiveBatch) { %>
              <% className = "time-slot big-orng-circel" %>
              <% } else { %>
              <% className = "time-slot checkmark-circel" %>
              <% } %>
              <div class="<%= className %>"> <%= getBatchTime(batch.timestamp) %> </div>
              <% }) %>
            </div>
          </div>
          <% } %>
          <div class="image-col">
            <div class="col-heading"> </div>
            <% stoppageHistory.frameRequests.forEach((frameRequest, frameRequestIndex) => { %>
            <% const batch = getBatch(frameRequest); %>
            <% if(!batch) return; %>
            <% const frames = batch.frames; %>
            <% const previousBatch = frameRequestIndex !== 0 ? getBatch(stoppageHistory.frameRequests[frameRequestIndex - 1]) : null; %>
            <% const nextBatch = frameRequestIndex < stoppageHistory.frameRequests.length - 1 ? getBatch(stoppageHistory.frameRequests[frameRequestIndex + 1]) : null; %>
            <% const isDefectiveBatch = batch.isBatchDefective; %>
            <% const isPreviousBatchDefective = previousBatch ? previousBatch.isBatchDefective : false; %>
            <% const isNextBatchDefective = nextBatch ? nextBatch.isBatchDefective : false; %>
            <div class="img-row">
              <% for(let index = 0; index<8; index++) { %>
              <% let className = "img-cover"; %>
              <% const frame = frames.find((frame) => frame.index === index) %>
              <% if(isDefectiveBatch && index === 7) { %>
              <% className+= " border-right" %>
              <% } %>
              <% if(isDefectiveBatch && index === 0) { %>
              <% className+= " border-left" %>
              <% } %>
              <% if(isDefectiveBatch && !isPreviousBatchDefective) {%>
                <% className+= " border-top" %>
              <% } %>
              <% if(isDefectiveBatch && !isNextBatchDefective) {%>
                <% className+= " border-bottom" %>
              <% } %>
              <div class="<%= className %>">
                <% if(frame) { %>
                <img src="<%= frame.url %>">
                <% } %>
              </div>
              <% } %>
            </div>
            <% }) %>
          </div>
          <div class="def-col">
            <div class="col-heading"> Rotation </div>
            <% stoppageHistory.frameRequests.forEach(frameRequest => { %>
            <% const batch = getBatch(frameRequest); %>
            <div class="col-txt"> <%= batch.rotationDelta ? batch.rotationDelta.toFixed() : batch.rotationDelta %>
            </div>
            <% }) %>
          </div>
          <div class="rpm-col">
            <div class="col-heading"> RPM </div>
            <% stoppageHistory.frameRequests.forEach(frameRequest => { %>
            <% const batch = getBatch(frameRequest); %>
            <div class="col-txt"> <%= batch.rotationSpeed ? batch.rotationSpeed.toFixed() : batch.rotationSpeed %>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
    </div>
    <% }) %>
    <% } %>


