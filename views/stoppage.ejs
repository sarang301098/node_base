  <%
    var getBatch = function(frameRequest) {
      const batches = frameRequest.batches;
      let batch = null;

      if(!batches.length) {
        return batch;
      }

      const lightTypes = ['IR', 'VIS', 'UV'];
      const lightPositions = ['TOP', 'FRONT'];
      let lightTypeIndex = 0;

      do {
        let lightPositionIndex = 0;
        const lightType = lightTypes[lightTypeIndex];
        let lightPosition = lightPositions[lightPositionIndex];
        batch = batches.find((batch) => batch.lightType === lightType && batch.lightPosition === lightPosition);

        if(!batch) {
          lightPositionIndex++;
          lightPosition = lightPositions[lightPositionIndex];
          batch = batches.find((batch) => batch.lightType === lightType && batch.lightPosition === lightPosition);
        }

        lightTypeIndex++;
      } while(!batch && lightTypeIndex < 3);

      return batch;
    }
  %>

  <% var isPreviousFrameDefective = function(index, previousBatch) {
    const prevBatchFrames = previousBatch.frames;
    const prevFrameForIndex = prevBatchFrames.find((frame) => frame.index === index);
    return prevFrameForIndex.isDefectiveFrame;
    }
  %>
  <% var isNextFrameDefective = function (index, nextBatch) {
    const nextBatchFrames = nextBatch.frames;
    const nextFrameForIndex = nextBatchFrames.find((frame) => frame.index === index);
    return nextFrameForIndex.isDefectiveFrame;
  }
